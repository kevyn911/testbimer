<div>
<%= f.label :firstname %>
<%= f.text_field :firstname, class: 'form-control',autofocus: true %>
</div>

<div>
<%= f.label :lastname %>
<%= f.text_field :lastname, class: 'form-control' %>
</div>

<div>
<%= f.label :country %>
<%= f.text_field :country, class: 'form-control' %>
</div>

<div>
<%= f.label :city %>
<%= f.text_field :city, class: 'form-control' %>
</div>

<div>
<%= f.label :avatar %>
<%= f.file_field :avatar%>
</div>

<div>
<%= f.label :phone %>
<%= f.text_field :phone, class: 'form-control' %>
</div>

<div>
<%= f.label :dofb %>
<%= f.date_field :dofb%>
</div>

<div>
<%= date_select("userparam", "dofb", start_year: 1950, end_year: 2000, order: [:month, :day, :year]) %>
</div>

<div>
<%= f.submit "Save", class: "btn btn-info" %>
</div>